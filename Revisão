
public class PersonTest {
	public static void main(String[] args)
	{
		Person p = new Person();
		NaturalPerson np = new NaturalPerson();
		
		np.setName("Vitor Lopes");
		np.setAdress("Londrina");
		np.setGrossIncome(2000);
		np.setSsn("12345");
		
		System.out.println(np.toString());
		System.out.println("Post-Taxes Income: " + np.taxationTheft());
	}
}
